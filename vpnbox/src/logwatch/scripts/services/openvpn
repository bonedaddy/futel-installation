while (defined($ThisLine = <STDIN>)) {
   if ( ( $ThisLine =~ /origin software="rsyslogd"/ ) ) {
       # we don't care about these
   } else {
       # Report any unmatched entries
       push @OtherList,$ThisLine;
   }
}

if ($#OtherList >= 0) {
   print "\n**Unmatched Entries**\n";
   print @OtherList;
}

exit(0);
