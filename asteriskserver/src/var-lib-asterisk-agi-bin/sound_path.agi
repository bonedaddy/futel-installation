#!/usr/bin/env python
"""
Return full path for given sound file name, without extension.
"""

import sys
from asterisk import *
import util

agi = agi.AGI()

def main():
    sound_path = util.sound_path(sys.argv[1])
    agi.set_variable('agi_out', sound_path)

agi.answer()
util.agi_tracebacker(agi, main)
#agi.hangup()
